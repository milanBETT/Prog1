using System;

namespace MyApplication
{
    public class Alpha
    {
        public static int randNum;
        ///Konstruktor ami random számokat hoz létre
        public Alpha()
        {
          Random rand = new Random();
          randNum = rand.Next(1, 100);                      
        }
    }
    public class Beta : Alpha ///Öröklés
    {
        
    }
}
